<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>


<GridPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.65">
   <rowConstraints>
      <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="50.0" />
      <RowConstraints maxHeight="50.0" minHeight="0.0" prefHeight="50.0" />
   </rowConstraints>
   <columnConstraints>
      <ColumnConstraints maxWidth="120.0" minWidth="10.0" prefWidth="120.0" />
      <ColumnConstraints maxWidth="120.0" minWidth="10.0" prefWidth="120.0" />
      <ColumnConstraints maxWidth="120.0" minWidth="10.0" prefWidth="120.0" />
   </columnConstraints>
   <children>
      <Button mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0" text="New Album">
         <GridPane.margin>
            <Insets />
         </GridPane.margin>
      </Button>
      <Button layoutX="10.0" layoutY="15.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0" text="Remove Album" GridPane.rowIndex="1" />
      <Button layoutX="10.0" layoutY="15.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0" text="Rename Album" GridPane.columnIndex="1" />
      <Button layoutX="10.0" layoutY="15.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0" text="Open Album" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Button layoutX="10.0" layoutY="15.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0" text="Log Out" GridPane.columnIndex="2" />
      <Button layoutX="10.0" layoutY="15.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="110.0" text="Exit" GridPane.columnIndex="2" GridPane.rowIndex="1" />
   </children>
</GridPane>
